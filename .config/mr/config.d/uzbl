[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/uzbl.git]
checkout = vcsh clone https://gitlab.stillwell.me/mark/vcsh-homedir-uzbl.git uzbl
setpush  = vcsh uzbl remote set-url --add --push origin git@stillwell.me:mark/vcsh-homedir-uzbl.git
           vcsh uzbl remote set-url --add --push origin git@github.com:marklee77/vcsh-homedir-uzbl.git
           vcsh uzbl remote set-url --add --push origin git@bitbucket.org:marklee77/vcsh-homedir-uzbl.git

